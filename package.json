    "date-fns": "^2.16.1",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "gulp-svgmin": "^4.0.1",
    "gulp-svgstore": "^8.0.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.14.1",
